{
  "name": "moleculer-realworld-example-app",
  "version": "1.1.0",
  "description": "RealWorld example app with Moleculer microservices framework",
  "scripts": {
    "dev": "moleculer-runner --repl --hot services",
    "start": "moleculer-runner",
    "deps": "npm-check -u",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint services",
    "docker:build": "docker build -t conduit ."
  },
  "keywords": [
    "microservices",
    "moleculer",
    "realworld"
  ],
  "author": "",
  "devDependencies": {
    "eslint": "4.18.1",
    "jest": "22.4.2",
    "jest-cli": "22.4.2",
    "moleculer-repl": "^0.3.0",
    "npm-check": "5.5.2"
  },
  "dependencies": {
    "bcrypt": "1.0.3",
    "jsonwebtoken": "8.1.1",
    "lodash": "4.17.5",
    "moleculer": "0.12.0-rc1",
    "moleculer-db": "0.7.0",
    "moleculer-db-adapter-mongo": "0.1.6",
    "moleculer-web": "github:moleculerjs/moleculer-web",
    "nats": "0.8.0",
    "slug": "0.9.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moleculerjs/moleculer-realworld-example-app.git"
  },
  "engines": {
    "node": ">= 6.x.x"
  },
  "jest": {
    "testEnvironment": "node",
    "rootDir": "./services",
    "roots": [
      "../test"
    ]
  }
}
