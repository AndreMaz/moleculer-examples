{
  "name": "moleculer-examples-conduit",
  "version": "3.0.0",
  "description": "RealWorld example app with Moleculer microservices framework",
  "scripts": {
    "dev": "moleculer-runner --repl --hot services",
    "start": "moleculer-runner",
    "deps": "npm-check -u",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint services",
    "docker:build": "docker build -t conduit ."
  },
  "keywords": [
    "microservices",
    "moleculer",
    "realworld"
  ],
  "author": "",
  "devDependencies": {
    "eslint": "6.0.1",
    "jest": "24.8.0",
    "jest-cli": "24.8.0",
    "moleculer-repl": "0.5.7",
    "npm-check": "5.9.0"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "ioredis": "4.11.1",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.11",
    "moleculer": "0.14.0-beta1",
    "moleculer-db": "0.8.0",
    "moleculer-db-adapter-mongo": "0.4.2",
    "moleculer-web": "0.9.0-beta3",
    "slug": "1.1.0"
  },
  "engines": {
    "node": ">= 8.x.x"
  },
  "jest": {
    "testEnvironment": "node",
    "rootDir": "./services",
    "roots": [
      "../test"
    ]
  }
}
