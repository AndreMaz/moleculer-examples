{
  "name": "moleculer-examples",
  "version": "1.0.0",
  "description": "Blog examples for Moleculer framework",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "docker:build": "docker build -t moleculer-blog .",
    "docker:run": "docker run --rm -it -p 3000:3000 moleculer-blog",

    "dc:mono": "docker-compose -f dc.mono.yml -d up",
    "dc:micro": "docker-compose -f dc.micro.yml -d up",
    "dc:mixed": "docker-compose -f dc.mixed.ym -d upl"
  },
  "author": "Ice Services",
  "license": "MIT",
  "dependencies": {
    "bluebird": "3.4.7",
    "body-parser": "1.16.1",
    "chalk": "1.1.3",
    "eslint": "3.16.1",
    "express": "4.14.1",
    "fakerator": "0.3.0",
    "lodash": "4.17.4",
    "moleculer": "ice-services/moleculer",
    "winston": "2.3.1"
  }
}
